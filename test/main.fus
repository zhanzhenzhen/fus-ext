        ass.notStrictEqual(b.b, a.b)
        c: {
            a: <>
            b: 7
        }
        c.a.m1: 5
        d: Object..deepClone(c)
        ass.deepStrictEqual(d, {a: {m1: 5}, b: 7})
    )
)

describe("timer", --
    it("IntervalTimer: normal", done ->
        ass.strictEqual(IntervalTimer.getPrecision(), 30)
        initTime: Date()
        timer: IntervalTimer{interval: 100}
        ass(0 ≤ timer.startTime - initTime < 20)
        count: 0
        timer.onArrive e ->
            now: Date()
            ass(e.idealTime is Date)
            ass(e.idealTime..equals(timer.startTime..add(count * 100)))
            ass(e.nowTime is Date)
            ass(0 ≤ now - e.nowTime < 20)
            ass(0 ≤ e.nowTime - e.idealTime < 50)
            ass.strictEqual(e.index, count)
            ass.strictEqual(timer.getCounter(), e.index + 1)
            ass.strictEqual(timer.getRunning(), true)
            count: self + 1
            if count = 5
                timer.stop()
                ass.strictEqual(timer.getRunning(), false)
                done()
    )
    it("IntervalTimer: skipsPast, startTime < now", done ->
        initTime: Date()
        timer: IntervalTimer{
            interval: 100
            startTime: initTime..subtract(200)
            endTime: initTime..add(300)
            skipsPast
        }
        count: 0
        timer.onArrive e ->
            now: Date()
            ass(e.idealTime is Date)
            if count = 0
                ass(e.idealTime..equals(initTime..subtract(200)))
            else if count = 1
                ass(e.idealTime..equals(initTime..add(100)))
            else if count = 2
                ass(e.idealTime..equals(initTime..add(200)))
            ass(e.nowTime is Date)
            ass(0 ≤ now - e.nowTime < 20)
            if count = 0
                ass(200 ≤ e.nowTime - e.idealTime < 250)
            else
                ass(0 ≤ e.nowTime - e.idealTime < 50)
            ass.strictEqual(e.index, count)
            ass.strictEqual(timer.getCounter(), e.index + 1)
            if count < 2
                ass.strictEqual(timer.getRunning(), true)
            else
                ass.strictEqual(timer.getRunning(), false)
            count: self + 1
            if count = 3
                done()
    )
    it("IntervalTimer: skipsPast, startTime < now, not includesStart, includesEnd", done ->
        initTime: Date()
        timer: IntervalTimer{
            interval: 100
            startTime: initTime..subtract(200)
            endTime: initTime..add(300)
            includesStart: false
            includesEnd
            skipsPast
        }
        count: 0
        timer.onArrive e ->
            now: Date()
            ass(e.idealTime is Date)
            if count = 0
                ass(e.idealTime..equals(initTime..add(100)))
            else if count = 1
                ass(e.idealTime..equals(initTime..add(200)))
            else if count = 2
                ass(e.idealTime..equals(initTime..add(300)))
            ass(e.nowTime is Date)
            ass(0 ≤ now - e.nowTime < 20)
            ass(0 ≤ e.nowTime - e.idealTime < 50)
            ass.strictEqual(e.index, count)
            ass.strictEqual(timer.getCounter(), e.index + 1)
            if count < 2
                ass.strictEqual(timer.getRunning(), true)
            else
                ass.strictEqual(timer.getRunning(), false)
            count: self + 1
            if count = 3
                done()
    )
    it("IntervalTimer: heavy work", done ->
        initTime: Date()
        timer: IntervalTimer{interval: 100}
        ass(0 ≤ timer.startTime - initTime < 20)
        count: 0
        timer.onArrive e ->
            now: Date()
            ass(e.idealTime is Date)
            ass(e.idealTime..equals(timer.startTime..add(count * 100)))
            ass(e.nowTime is Date)
            ass(0 ≤ now - e.nowTime < 20)
            if count = 0
                ass(280 ≤ e.nowTime - e.idealTime < 350)
            else if count = 1
                ass(210 ≤ e.nowTime - e.idealTime < 280)
            else if count = 2
                ass(140 ≤ e.nowTime - e.idealTime < 210)
            else if count = 3
                ass(70 ≤ e.nowTime - e.idealTime < 140)
            else
                ass(0 ≤ e.nowTime - e.idealTime < 70)
            ass.strictEqual(e.index, count)
            ass.strictEqual(timer.getCounter(), e.index + 1)
            ass.strictEqual(timer.getRunning(), true)
            count: self + 1
            if count = 5
                timer.stop()
                ass.strictEqual(timer.getRunning(), false)
                done()
        sleep(300)
    )
    it("IntervalTimer: heavy work, skipsPast", done ->
        initTime: Date()
        timer: IntervalTimer{interval: 100, skipsPast}
        ass(0 ≤ timer.startTime - initTime < 20)
        count: 0
        timer.onArrive e ->
            now: Date()
            ass(e.idealTime is Date)
            if count = 0
                ass(e.idealTime..equals(timer.startTime))
            else if count = 1
                ass(e.idealTime..equals(timer.startTime..add(300)))
            else if count = 2
                ass(e.idealTime..equals(timer.startTime..add(400)))
            ass(e.nowTime is Date)
            ass(0 ≤ now - e.nowTime < 20)
            if count = 0
                ass(230 ≤ e.nowTime - e.idealTime < 300)
            else
                ass(0 ≤ e.nowTime - e.idealTime < 50)
            ass.strictEqual(e.index, count)
            ass.strictEqual(timer.getCounter(), e.index + 1)
            ass.strictEqual(timer.getRunning(), true)
            count: self + 1
            if count = 3
                timer.stop()
                ass.strictEqual(timer.getRunning(), false)
                done()
        sleep(250)
    )
    it("Observer: change", done ->
        value: 1
        Observer(<> value).onChange(e ->
            ass.strictEqual(e.oldValue, 1)
            ass.strictEqual(e.newValue, 2)
            done()
        )
        setTimeout(<> value: 2, 200)
    )
    it("Observer: update", done ->
        count: 0
        value: 1
        Observer(<> value).onUpdate(e ->
            if count = 0
                ass.strictEqual(e.value, 2)
                count: self + 1
            else
                ass.strictEqual(e.value, 3)
                done()
        )
        value: 2
        setTimeout(<> value: 3, 200)
    )
    it("Observer: manual update", --
        updateCount: 0
        changeCount: 0
        value: 10
        observer: Observer(<> value).onUpdate(e ->
            updateCount: self + 1
        ).onChange(e ->
            changeCount: self + 1
        )
        ass.strictEqual(updateCount, 0)
        ass.strictEqual(changeCount, 0)
        value: 20
        observer.check()
        ass.strictEqual(updateCount, 1)
        ass.strictEqual(changeCount, 0)
        value: 30
        observer.check()
        ass.strictEqual(updateCount, 2)
        ass.strictEqual(changeCount, 1)
        observer.check()
        ass.strictEqual(updateCount, 2)
        ass.strictEqual(changeCount, 1)
        observer.check(true)
        ass.strictEqual(updateCount, 3)
        ass.strictEqual(changeCount, 2)
    )
)

